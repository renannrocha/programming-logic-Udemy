Algoritmo "semnome"

Var

 distancia, combustivel, consumo : real
Inicio
 escreval("-----calculo de consumo-----")
 escreval
 escreva("Distancia percorrida: ")
 leia(distancia)
 escreva("Combustível gasto: ")
 leia(combustivel)
 
 consumo <- distancia / combustivel
 
 escreval("Combustível gasto: ", consumo:8:3)

Fimalgoritmo