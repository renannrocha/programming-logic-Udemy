Algoritmo "soma"

Var
  x, y, soma: inteiro

Inicio
  escreval("----------soma de valores------------")

  escreva("digite o valor de X: ")
  leia(x)
  escreva("digite o valor de Y: ")
  leia(y)
  
  soma <- x + y
  
  
  escreval("-------------------------------------")
  
  escreval("o valor da soma entre", x, " e", y, " e igual a", soma)



Fimalgoritmo